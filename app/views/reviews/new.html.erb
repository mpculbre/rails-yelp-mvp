<div class="card" style="padding: 2em"> 
<h1 class="text-center">Create a new review</h1> 
  <%= simple_form_for [@restaurant, @review] do |f| %>
    <%= f.input :content %>
    <%= f.input :rating %>
    <%= f.submit "add a review", class: "btn btn-primary" %>
  <% end %>

<%= link_to "home", restaurants_path %>
</div>